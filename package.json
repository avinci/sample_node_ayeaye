{
  "name": "vb-docker-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "mocha --recursive \"tests/**/*.spec.js\"",
    "coverage": "istanbul --include-all-sources cover -root \"routes\" --dir shippable/codecoverage node_modules/mocha/bin/_mocha -- -R spec-xunit-file --recursive \"tests/**/*.spec.js\" && mv xunit.xml shippable/testresults/"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "ip": "^1.1.4",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0"
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "spec-xunit-file": "0.0.1-3",
    "supertest": "^2.0.1"
  }
}
